# ==========================================
# SaaS Sesli Asistan - Environment Variables
# ==========================================

# Twilio Credentials
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Deepgram API (Speech-to-Text & TTS)
DEEPGRAM_API_KEY=your_deepgram_api_key

# OpenAI API (Conversation AI)
OPENAI_API_KEY=your_openai_api_key

# ElevenLabs API (Premium TTS - Optional)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=your_voice_id

# Supabase (Database & Auth)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key  # For admin operations

# Server Configuration
PORT=3000
NODE_ENV=development
SERVER_URL=https://your-ngrok-url.ngrok.io

# CORS (Frontend URL'leri, virgülle ayrılmış)
CORS_ORIGIN=http://localhost:5173,http://localhost:3000

# Internal API Key (webhook'lar için)
INTERNAL_API_KEY=your_internal_api_key

# ==========================================
# Frontend Environment Variables
# (frontend/.env dosyasına kopyalayın)
# ==========================================
# VITE_API_URL=http://localhost:3000
# VITE_APP_NAME=SaaS Sesli Asistan
